   "source": [
    "import re\n",
    "\n",
    "def after(match):\n",
    "    value = int(match.group())\n",
    "    value *= value\n",
    "    result = str(value)\n",
    "    return result\n",
    "        \n",
    "p = re.compile(r'\\d+')\n",
    "p.sub(after, \"Please, square the following nnumbers, 3 7 11 13 17 19\")"
   ]
